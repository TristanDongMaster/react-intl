{
    "presets": [
        "es2015",
        "react",
        "stage-0",
        "flow"
    ],
    "env": {
        "development": {
            "presets": [
                "react-hmre"
            ]
        }
    },
    "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "syntax-dynamic-import", [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": true
            }
        ]
    ]
}